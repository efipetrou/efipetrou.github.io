{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": [
		"src/**/*",
		"src/node_modules/**/*"
	],
	"exclude": [
		"node_modules/*",
		"__sapper__/*",
		"public/*"
	],
	"compilerOptions": {
		"lib": [
			"DOM",
			"ES2017",
			"WebWorker"
		],
		"allowJs": true,
		"checkJs": false,
		"strict": false,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"baseUrl": "./",
		"paths": {},
		"typeRoots": [
			"typings"
		],
		"types": [
			"svelte",
			"@sapper",
			"node"
		],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	}
}