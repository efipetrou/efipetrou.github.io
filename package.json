{
  "name": "efipetrou.nl",
  "description": "Portfolio for Efi Petrou Web Designer",
  "keywords": [
    "Portfolio",
    "Web Designer",
    "UX Designer",
    "UI Designer"
  ],
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:tailwind sapper-dev",
    "build": "yarn run build:tailwind && sapper build --legacy",
    "export": "yarn run build:tailwind && sapper export docs --legacy",
    "start": "node __sapper__/build",
    "sapper-dev": "sapper dev",
    "watch:tailwind": "./node_modules/postcss-cli/bin/postcss src/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production ./node_modules/postcss-cli/bin/postcss src/tailwind.css -o static/index.css"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.35",
    "@types/polka": "^0.5.2",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.29.1",
    "sass": "^1.32.8",
    "svelte": "^3.24.0",
    "svelte-preprocess": "^4.6.9",
    "svelte-scrollto": "^0.2.0",
    "tailwindcss": "^2.0.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
